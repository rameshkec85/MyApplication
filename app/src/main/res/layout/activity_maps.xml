<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.testapp.myapplication.SlidingUpDashboard xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="bottom"
        app:dashboardBody="@layout/dashboard_sliding_body"
        app:dashboardMainContent="@layout/test"
        app:dashboardPanelHeight="95dp"
        app:dashboardParalaxOffset="210dp"
        app:dashboardShadowHeight="0dp"
        app:umanoDragView="@+id/img_avatar"
        app:umanoFadeColor="@android:color/transparent"
        app:umanoInitialState="collapsed"
        app:umanoOverlay="true" />

</FrameLayout>